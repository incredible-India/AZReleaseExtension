{
"manifest_version":3,
"name":"AZRelease",
"version":"1.0.0",
"description":"AZRelease extesnion provide facilities to copy the existing release (all builds) and paste it same builds for the new release. Applicable only for Azure Deops (HKS)",

"permissions":["storage",
                "notifications",
                "tabs",
                "activeTab",
                "contextMenus"
            ],
"icons":{
    "16":"./icons/16.png"
},
"action":{
    "default_popup":"./html/popup.html",
    "default_icon":"./icons/16.png",
    "default_title":"AZ-Release"
},

"background":{
     "service_worker": "./js/ReleaseEvent.js"
},
"content_scripts":[
{
    "matches":["https://dev.azure.com/incadeadev/*"],
    "js":["./js/content.js"]
   
}
]

}